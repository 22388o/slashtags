{
  "name": "@synonymdev/slashtags-identity",
  "version": "0.0.1",
  "description": "Slashtags decentralized identity management",
  "type": "module",
  "keywords": [
    "did"
  ],
  "homepage": "https://github.com/synonymdev/slashtags/tree/master/packages/identity/#readme",
  "bugs": {
    "url": "https://github.com/synonymdev/slashtags/issues"
  },
  "license": "MIT",
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "types": "types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "main": "src/index.js",
  "browser": {
    "./src/storage/storage.js": "./src/storage/storage.browser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synonymdev/slashtags.git"
  },
  "scripts": {
    "build": "aegir build",
    "test": "c8 aegir test",
    "lint": "aegir ts -p check && aegir lint --fix",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check",
    "augment": "cat types/src/interfaces.d.ts types/src/index.d.ts > ./dist/types/src/index.d.ts"
  },
  "devDependencies": {
    "aegir": "^36.1.3",
    "did-resolver": "^3.1.5"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "b4a": "^1.3.1",
    "compact-encoding": "^2.6.1",
    "compact-encoding-maybe-enum": "^1.0.2",
    "compact-encoding-struct": "^1.2.1",
    "corestore": "^6.0.1-alpha.9",
    "dht-universal": "^0.3.2",
    "hyperswarm": "^3.0.1",
    "multiformats": "^9.6.4",
    "random-access-memory": "^4.0.0",
    "sodium-universal": "^3.1.0"
  }
}