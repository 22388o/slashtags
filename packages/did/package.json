{
  "name": "@synonymdev/slashtags-did",
  "version": "0.0.0",
  "description": "Slashtags DID management",
  "type": "module",
  "keywords": [
    "did"
  ],
  "homepage": "https://github.com/synonymdev/slashtags/tree/master/packages/did/#readme",
  "bugs": {
    "url": "https://github.com/synonymdev/slashtags/issues"
  },
  "license": "MIT",
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "types": "types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "main": "src/index.js",
  "browser": {
    "./src/hyper/storage.js": "./src/hyper/storage.browser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synonymdev/slashtags.git"
  },
  "scripts": {
    "build": "aegir build",
    "test": "c8 aegir test",
    "lint": "aegir ts -p check && aegir lint --fix",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check"
  },
  "devDependencies": {
    "aegir": "^36.1.3",
    "did-resolver": "^3.1.5"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "b4a": "^1.3.1",
    "compact-encoding": "^2.6.1",
    "compact-encoding-struct": "^1.2.1",
    "corestore": "^6.0.1-alpha.9",
    "dht-universal": "^0.1.1",
    "hyperbee": "^1.7.0",
    "hyperswarm": "^3.0.1",
    "random-access-memory": "^4.0.0",
    "turbo-hash-map": "^1.0.2"
  }
}
