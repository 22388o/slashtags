{
  "name": "@synonymdev/slashtags-core",
  "version": "0.1.1",
  "description": "JavaScript implementation of the Slashtags protocol",
  "type": "module",
  "keywords": [
    "slashtags"
  ],
  "homepage": "https://github.com/synonymdev/slashtags/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/synonymdev/slashtags/issues"
  },
  "license": "MIT",
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "types": "types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synonymdev/slashtags.git"
  },
  "scripts": {
    "build": "aegir build",
    "test": "c8 aegir test",
    "lint": "aegir ts -p check && aegir lint --fix",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check"
  },
  "dependencies": {
    "dht-universal": "^0.0.8",
    "jsonrpc-lite": "^2.2.0"
  },
  "devDependencies": {
    "aegir": "^36.1.3"
  },
  "peerDependencies": {
    "b4a": "^1.3.1"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
