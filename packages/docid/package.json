{
  "name": "@synonymdev/slashtags-docid",
  "version": "0.0.1-alpha.0",
  "description": "Slashtags Dcoument IDs",
  "author": "Ar Nazeh <ar.nazeh@gmail.com>",
  "main": "./src/index.js",
  "types": "./types/index.d.ts",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "types/*"
      ],
      "types/*": [
        "types/*"
      ]
    }
  },
  "scripts": {
    "lint": "standard  --fix ./src",
    "build:js": "ipjs build --tests --main",
    "build:types": "tsc -p tsconfig.json",
    "build": "npm run build:js && npm run build:types",
    "test": "hundreds mocha test --recursive",
    "test:cjs:browser": "cd ../../ && polendina --page --worker --serviceworker --cleanup packages/docid/dist/cjs/browser-test/*.spec.js",
    "test:cjs": "npm run build && mocha dist/cjs/node-test/*.spec.js",
    "test:all": "npm run lint && npm run test && npm run test:cjs && npm run test:cjs:browser",
    "prepublishOnly": "npm run test:all && rm -rf dist/**/*test"
  },
  "keywords": [
    "slashtags",
    "identifiers",
    "cid",
    "feed"
  ],
  "dependencies": {
    "multiformats": "^9.4.6"
  },
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./DocID": {
      "import": "./src/DocID.js"
    },
    "./constants": {
      "import": "./src/constants.js"
    },
    "./docTypes/CID": {
      "import": "./src/docTypes/CID.js"
    }
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "dist/"
    ]
  }
}