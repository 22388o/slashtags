{
  "name": "@synonymdev/slashtags-docid",
  "version": "0.0.0",
  "description": "Slashtags Dcoument IDs",
  "author": "Ar Nazeh <ar.nazeh@gmail.com>",
  "main": "./src/index.js",
  "types": "./types/index.d.ts",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "eslint ./src --fix",
    "build:copy": "cp -a src tsconfig.json dist/",
    "build:js": "ipjs build --tests --main && npm run build:copy",
    "build:types": "tsc -p tsconfig.json && mv ./types dist/types",
    "build": "npm run build:js && npm run build:types",
    "test": "hundreds mocha test/**/*.spec.js",
    "test:cjs:browser": "cd ../../ && polendina packages/docid/dist/cjs/browser-test/*.spec.js",
    "test:cjs": "npm run build:js && mocha dist/cjs/node-test/*.spec.js && npm run test:cjs:browser",
    "test:all": "npm run lint && npm run test && npm run test:cjs",
    "prepublishOnly": "npm run test:all && rm -rf dist/**/*test"
  },
  "keywords": [
    "slashtags",
    "identifiers",
    "cid",
    "feed"
  ],
  "dependencies": {
    "multiformats": "^9.4.6"
  },
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./DocID": {
      "import": "./src/DocID.js"
    },
    "./constants": {
      "import": "./src/constants.js"
    },
    "./docTypes/CID": {
      "import": "./src/docTypes/CID.js"
    }
  }
}